{
  "name": "@violice/rmu",
  "version": "0.2.5",
  "author": "Sergey Ivashko",
  "license": "MIT",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "start": "tsdown --watch",
    "build": "tsdown",
    "prepare": "tsdown",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-native": ">=0.60"
  },
  "peerDependenciesMeta": {
    "react-native": {
      "optional": true
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "react-native": "dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "sideEffects": false,
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "2 KB"
    },
    {
      "path": "dist/index.mjs",
      "limit": "2 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^11.2.0",
    "@types/react": "^18.3.25",
    "@types/react-dom": "^18.3.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "size-limit": "^11.2.0",
    "tsdown": "^0.15.6",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3"
  }
}
