{
  "version": "0.2.5",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdown --watch",
    "build": "tsdown",
    "prepare": "tsdown",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@violice/rmu",
  "author": "Sergey Ivashko",
  
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "sideEffects": false,
  "size-limit": [
    { "path": "dist/index.js", "limit": "10 KB" },
    { "path": "dist/index.mjs", "limit": "10 KB" }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.8",
    "@types/react": "^18.2.25",
    "@types/react-dom": "^18.2.11",
    "husky": "^7.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "size-limit": "^7.0.8",
    "tsdown": "^0.15.5",
    "tslib": "^2.6.2",
    "typescript": "^5.9.2"
  }
}
